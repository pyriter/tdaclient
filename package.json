{
  "name": "tdaclient",
  "version": "1.3.0",
  "description": "TD Ameritrade Client",
  "main": "dist/main.js",
  "scripts": {
    "test": "jest --silent --detectOpenHandles --coverage --runInBand",
    "test-dev": "jest --watch --silent --detectOpenHandles --coverage",
    "build": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/pyriter/tdaclient.git"
  },
  "author": "Phong Vuong",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@babel/runtime": "^7.12.5",
    "@jest/globals": "^26.6.2",
    "axios": "^0.24.0",
    "core-js": "2",
    "https": "^1.0.0",
    "qs": "^6.9.4",
    "querystring": "^0.2.0",
    "source-map-loader": "^3.0.0",
    "ts-loader": "^9.2.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-modules-amd": "^7.12.1",
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@types/jest": "^26.0.24",
    "babel-jest": "^26.0.1",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.5",
    "webpack": "^5.44.0",
    "webpack-cli": "^4.7.2"
  },
  "jest": {
    "collectCoverage": true,
    "clearMocks": true,
    "resetMocks": true,
    "resetModules": true,
    "testEnvironment": "node",
    "coverageReporters": [
      "html"
    ],
    "testTimeout": 300000,
    "roots": [
      "<rootDir>/src"
    ],
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)",
      "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  },
  "bugs": {
    "url": "https://github.com/pyriter/tdaclient/issues"
  },
  "homepage": "https://github.com/pyriter/tdaclient#readme",
  "keywords": []
}
